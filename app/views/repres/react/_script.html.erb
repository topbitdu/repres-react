<% options = defined?(options) ? options : nil %>

<%= javascript_include_tag "//#{domain_name(:react, options)}/react/#{library_version(:react, '15.5.4', options)}/react.min.js" if library_enabled?(:react, options) %>
<%= javascript_include_tag "//#{domain_name(:'react-dom', options)}/react/#{library_version(:'react-dom', '15.5.4', options)}/react-dom.min.js" if library_enabled?(:'react-dom', options) %>

<%= javascript_include_tag "//#{domain_name(:redux, options)}/redux/#{library_version(:redux, '3.6.0', options)}/redux.min.js" if library_enabled?(:redux, options) %>
